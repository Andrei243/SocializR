@model PhotoModel

    <form enctype="multipart/form-data" asp-controller="Profile" asp-action="AddPhoto">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="Binar" class="control-label" id="butonAdaugarePoza">Add a photo</label>
            <input type="file" asp-for="Binar" class="form-control" hidden />
            <img src="#" id="imgPreview" />
            <span asp-validation-for="Binar" class="text-danger"></span>
        </div>
            <input type="number" asp-for="AlbumId" value="@Model.AlbumId" hidden />
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-primary" />
        </div>
    </form>