@model IEnumerable<ASP.NET_Core_UI.Models.DomainModels.County>

@{
    Layout = "~/Views/Shared/GeneralLayout.cshtml";

}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody id="countyBody">
        
    </tbody>
</table>

<script id="county-template" type="text/x-handlebars-template">
        <tr>
            <td>
                {{name}}
            </td>
            <td>
                <a href="/Counties/Edit/{{id}}" >Edit</a> |
                <a href="/Counties/Details/{{id}}">Details</a> |
                <a class="needConfirmation" data-id="{{id}}" href="#" onclick="return false">Delete</a>
            </td>


        </tr>

</script>

<button id="countyGetter">Load more counties</button>

<script src="~/js/County.js"></script>